class Book 
{
	private String title;
	private double price;

	public Book(String title, double price){
		this.title = title;
		this.price = price;
	}

	public String toString(){
		return "书名：" + this.title + "\t价格：" + this.price;
	}

	public void fun(){
		System.out.println(this);
		System.out.println(super.toString());
	}
}
